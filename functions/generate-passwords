generate-passwords() {
  if [ "$#" -ne 1 ]; then
    echo "Usage: generate-password <amount of characters>"
    echo "Assuming 64 characters:\n"
    pass_lenght=64
  else
    pass_lenght=${0}
  fi

  for i in {1..5};
    do (tr -cd '[:alnum:]' < /dev/urandom | fold -w${pass_lenght}| head -n 1);
  done
}
