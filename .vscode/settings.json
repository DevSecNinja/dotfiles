{
    "git.autofetch": true,
    "git.autofetchPeriod": 180,
    "git.postCommitCommand": "sync",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "files.associations": {
        "*.ps1.tmpl": "powershell",
        "*.sh.tmpl": "shellscript"
    },
    "pester.testFilePath": [
        "tests/powershell/*.Tests.ps1"
    ],
    "powershell.pester.codeLens": false
}